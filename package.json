{
  "name": "electron-boilerplate",
  "version": "1.0.0",
  "description": "Electron Boilerplate",
  "main": "public/main.js",
  "scripts": {
    "start-front": "webpack-dev-server --config ./webpack.config.front.js --mode development",
    "build-front": "webpack --config ./webpack.config.front.js --mode production",
    "start-back": "start nodemon --exec babel-node -- server/index.js",
    "build-back": "webpack --config ./webpack.config.back.js --mode production",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"npm run start-back && npm run start-front\" \"wait-on http-get://localhost:8080 && npm run electron-start\"",
    "preelectron-pack": "npm run build-front && npm run build-back",
    "electron-pack": "electron-builder --config ./config-electron.json",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint webapp"
  },
  "keywords": [],
  "author": "Vuka",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "dotenv-webpack": "^1.7.0",
    "electron": "^9.4.0",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.10.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "react-hot-loader": "^4.8.3",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "electron-is-dev": "^1.0.1",
    "express": "^4.16.4",
    "jquery": "^3.5.0",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.0.6"
  }
}
